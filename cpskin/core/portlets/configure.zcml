<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="cpskin">

  <browser:resource
      name="cpskin.portlets.firefox.png"
      file="firefox.png"
      />

  <browser:page
      name="portlet_firefox"
      for="*"
      template="firefox.pt"
      permission="zope.Public"
      layer="..interfaces.ICPSkinCoreLayer"
      />

</configure>

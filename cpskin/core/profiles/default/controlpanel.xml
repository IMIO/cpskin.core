<?xml version="1.0"?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="cpskin"
        purge="False">

    <configlet
        title="CPSkin"
        action_id="cpskin.settings"
        appId="cpskin.core"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@cpskin-controlpanel"
        icon_expr="string:${portal_url}/maintenance_icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>CPSkin: Site administrator</permission>
    </configlet>

</object>
